<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.zwjgsgoa.model.UserModel" table="zg_user">
		<id name="user_id">
			<generator class="assigned"></generator>
		</id>
		<property name="user_pwd"/>
		<property name="user_limit"/>
		<property name="user_name"/>
		<property name="user_gender"/>
		<property name="user_mobile"/>
		<property name="user_phone"/>
		<property name="user_email"/>
		<property name="user_birthday"/>
		<property name="user_status"/>
		<property name="user_depement"/>
	</class>
</hibernate-mapping>